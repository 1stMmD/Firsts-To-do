{
    
    "manifest_version" : 3,
    "name" : "First things todo - Todolists & Notes",
    "version" : "1.0",
    "description" : "Create todo lists and take notes with a simple and quick way.",
    "icons" : 
    {
        "128" : "icons/app-logo-128.png",
        "48" : "icons/app-logo-48.png",
        "16" : "icons/app-logo-16.png"
    },
    "action":
    {
        "default_popup" : "Public/index.html"
    },
    "content_security_policy": {
        "extension_page": "script-src 'self' 'unsafe-inline' https://example.com; object-src 'self'"
      } 
}